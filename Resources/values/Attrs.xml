<?xml version="1.0" encoding="utf-8" ?>
<resources>
  <declare-styleable name="GalTextView">
    <attr name="textContent" format="string"></attr>
    <attr name="textSize" format="dimension"></attr>
    <attr name="textColor" format="color"></attr>
    <attr name="textSpacingMult" format="float"></attr>
    <attr name="textSpacingAdd" format="float"></attr>
    <attr name="textXAlignment" format="string"></attr>
  </declare-styleable>

  <attr name="color_no_finger_inner_circle" format="color" />
  <attr name="color_no_finger_outer_circle" format="color" />
  <attr name="color_finger_on" format="color" />
  <attr name="color_finger_up" format="color" />
  <attr name="count" format="integer" />
  <attr name="tryTimes" format="integer" />
  
  <declare-styleable name="GestureLockViewGroup">
    <attr name="color_no_finger_inner_circle" />
    <attr name="color_no_finger_outer_circle" />
    <attr name="color_finger_on" />
    <attr name="color_finger_up" />
    <attr name="count" />
    <attr name="tryTimes" />
  </declare-styleable>

  <declare-styleable name="MaxHeightListView">
    <attr name="maxHeight" format="float"></attr>
  </declare-styleable>

</resources>